#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andrelademann/Development/vergissberlin/codestats-readme/node_modules/sshpk/bin/node_modules:/Users/andrelademann/Development/vergissberlin/codestats-readme/node_modules/sshpk/node_modules:/Users/andrelademann/Development/vergissberlin/codestats-readme/node_modules:/Users/andrelademann/Development/vergissberlin/node_modules:/Users/andrelademann/Development/node_modules:/Users/andrelademann/node_modules:/Users/node_modules:/node_modules:/Users/andrelademann/Development/vergissberlin/codestats-readme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andrelademann/Development/vergissberlin/codestats-readme/node_modules/sshpk/bin/node_modules:/Users/andrelademann/Development/vergissberlin/codestats-readme/node_modules/sshpk/node_modules:/Users/andrelademann/Development/vergissberlin/codestats-readme/node_modules:/Users/andrelademann/Development/vergissberlin/node_modules:/Users/andrelademann/Development/node_modules:/Users/andrelademann/node_modules:/Users/node_modules:/node_modules:/Users/andrelademann/Development/vergissberlin/codestats-readme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-conv" "$@"
else
  exec node  "$basedir/../sshpk/bin/sshpk-conv" "$@"
fi
